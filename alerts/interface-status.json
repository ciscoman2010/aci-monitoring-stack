apiVersion: 1
groups:
    - orgId: 1
      name: ACI
      folder: ACI
      interval: 1m
      rules:
        - uid: cdpwo14v5zzlsc
          title: Interface State Change
          condition: B
          data:
            - refId: A
              relativeTimeRange:
                from: 600
                to: 0
              datasourceUid: prometheus
              model:
                editorMode: code
                exemplar: false
                expr: changes(aci_interface_oper_state{interface_type="phys"}[$__rate_interval])>0
                instant: true
                intervalMs: 1000
                legendFormat: __auto
                maxDataPoints: 43200
                range: false
                refId: A
            - refId: B
              relativeTimeRange:
                from: 600
                to: 0
              datasourceUid: __expr__
              model:
                conditions:
                    - evaluator:
                        params:
                            - 0
                            - 0
                        type: gt
                      operator:
                        type: and
                      query:
                        params: []
                      reducer:
                        params: []
                        type: avg
                      type: query
                datasource:
                    name: Expression
                    type: __expr__
                    uid: __expr__
                expression: A
                intervalMs: 1000
                maxDataPoints: 43200
                refId: B
                type: threshold
          dashboardUid: f8fcf6a0-2c46-42cb-a41f-689e6865ce3f
          panelId: 25
          noDataState: NoData
          execErrState: Error
          for: 0s
          labels: {}
